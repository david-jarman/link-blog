{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Feed": {
    "MaxPostCount": 20
  },
  "PostStoreOptions": {
    "EnableInMemoryCache": true,
    "CacheRefreshInterval": "00:30:00"
  },
  "ImageCleanupOptions": {
    "EnableCleanup": true,
    "CleanupInterval": "01:00:00"
  }
}
